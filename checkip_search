    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.2.1/jquery.min.js"></script>
    <script>
        
        
     $(document).ready(function () {
        
         $('button').click(function(){
            
            var sdata = $('#name').val();
            //alert(sdata);
            var flag = false;
             
               $('.test tr:first td').each(function(k, v) {
                    $('.test tr').each(function() {
                    var $table = $(this);
                    var data2 =$table.find("td").eq(k).html();

                     //alert(data2); 

                        if ($.trim(data2) == $.trim(sdata)){
                             flag = true;
                             //alert("search"); 
                        }
                    });
                });

             if (flag){
                 $( "#result" ).empty();
                 $( "#result" ).append( "This data is exist" );
             }else{
                 $( "#result" ).empty();
                 $( "#result" ).append( "There is no data" );
             }
             
        });
     });    
        
 
    </script>    
        
    </head>

    <body>
       <div id="content">
         <h1>This is IP(Web) infomation file</h1>
       </div>
        
        <div id="content">
            <h1>serch</h1><input type="text" id = "name" name="name" value="52.26.14.11">
            <button type="button">Find</button>
            
            <div id="result"></div>
        </div>
 
<?php



//read file
$myfile = fopen("list.txt", "r") or die("Unable to open file!");
// Output one line until end-of-file
$lastdata = "";

$ipdata = array();
$webdata = array();
           
           
while(!feof($myfile)) {
    
    $line = fgets($myfile);
    //echo "substring=>".substr($line,0,3)."</br>";
    
    $cmp =strcmp(substr($line,0,5), "https");
    //echo "compare value=>".substr($line,0,5)."==>".$cmp."</br>";
    
    //http 주소임
    if ($cmp == 0){
        array_push($webdata,$line);
    }else{
        
        if ($line.length > 1){
            array_push($ipdata,$line);
        }
    }
        
    
}
fclose($myfile);

//print_r("webdata: ");
//print_r($webdata);
//
//print_r("ipdata: ");
//print_r($ipdata);

   echo "<div id='content'><table class='test' border='1'>";
        $i= 0;
        foreach ($ipdata as $ip) {
            echo "<tr><td id='std'>".$ip."</td>";
            echo "<td id='std'><a href='".$webdata[$i]."'>$webdata[$i]</td></tr>";
            $i++;
           
        }
   echo "</table></div>";
           
    
        
?>

     
            
    </body>
</html>           

